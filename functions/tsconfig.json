{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "compileOnSave": true,
  "include": ["next-env.d.ts", "src"],
  "exclude": ["node_modules"]
}
